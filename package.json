    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
