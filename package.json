    "express": "^4.21.2",
    "highlight.js": "^11.11.1",
    "idb": "^8.0.3",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "murmurhash": "^2.0.1",
    "ng-zorro-antd": "^20.1.3",
    "ngx-clipboard": "^16.0.0",
    "rxjs": "^7.8.2",
    "sitemap": "^8.0.0",
    "tslib": "^2.8.1",
    "ua-parser-js": "^2.0.4",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.2",
    "@types/express": "^4.17.23",
    "@types/jasmine": "^5.1.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/ua-parser-js": "^0.7.39",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "jasmine-core": "^5.9.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "less": "^4.4.1",
    "ng-packagr": "^20.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
