  },
  "bugs": {
    "url": "https://github.com/ifuyun/ifuyun.com/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:gulp": "gulp build",
    "build:prod": "npm run build && npm run build:gulp",
    "build:zip": "bash ./scripts/zip.sh",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node dist/server/server.mjs",
    "lint": "eslint \"src/**/*.{ts,html}\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy": "bash ./scripts/deploy.sh",
    "deploy:local": "bash ./scripts/deploy-local.sh",
    "deploy:manual": "bash ./scripts/deploy-manual.sh",
    "cloc": "cloc --exclude-dir=.git,.idea,.vscode,node_modules,dist,test,package-lock.json,.angular,.github,.nx ."
  },
  "dependencies": {
    "@angular/animations": "^19.0.6",
    "@angular/common": "^19.0.6",
    "@angular/compiler": "^19.0.6",
    "@angular/core": "^19.0.6",
    "@angular/forms": "^19.0.6",
    "@angular/platform-browser": "^19.0.6",
    "@angular/platform-browser-dynamic": "^19.0.6",
    "@angular/platform-server": "^19.0.6",
    "@angular/router": "^19.0.6",
    "@angular/ssr": "^19.0.7",
    "@fuyun/feed": "^1.0.0",
    "express": "^4.21.2",
    "highlight.js": "^11.11.1",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "murmurhash": "^2.0.1",
