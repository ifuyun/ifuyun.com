    "Wutong CMS",
    "Angular",
    "Angular SSR",
    "SSR",
    "Fuyun"
  ],
  "author": {
    "name": "Fuyun",
    "url": "https://www.ifuyun.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ifuyun/ifuyun.com#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifuyun/ifuyun.com.git"
  },
  "bugs": {
    "url": "https://github.com/ifuyun/ifuyun.com/issues"
  },
  "scripts": {
    "ng": "ng",
    "start:www": "ng serve www",
    "start:blog": "ng serve blog",
    "start:wallpaper": "ng serve wallpaper",
    "start:jigsaw": "ng serve jigsaw",
    "start:game": "ng serve game",
    "build": "bash scripts/build-ng.sh",
    "build:win": ".\\scripts\\build-ng.bat",
    "build:common": "ng build common",
    "build:common:styles": "gulp common:build",
    "build:www": "ng build www",
    "build:blog": "ng build blog",
    "build:wallpaper": "ng build wallpaper",
    "build:jigsaw": "ng build jigsaw",
    "build:game": "ng build game",
    "build:gulp": "gulp build",
    "build:gulp:www": "gulp gzip:www",
    "build:gulp:blog": "gulp gzip:blog",
    "build:gulp:wallpaper": "gulp gzip:wallpaper",
    "build:gulp:jigsaw": "gulp gzip:jigsaw",
    "build:gulp:game": "gulp gzip:game",
    "build:prod": "npm run build && npm run build:gulp",
    "build:win:prod": "npm run build:win && npm run build:gulp",
    "build:prod:www": "npm run build:www && npm run build:gulp:www",
    "build:prod:blog": "npm run build:blog && npm run build:gulp:blog",
    "build:prod:wallpaper": "npm run build:wallpaper && npm run build:gulp:wallpaper",
    "build:prod:jigsaw": "npm run build:jigsaw && npm run build:gulp:jigsaw",
    "build:prod:game": "npm run build:game && npm run build:gulp:game",
    "build:zip": "bash scripts/build-zip.sh",
    "build:zip:www": "bash scripts/zip.sh --app=www",
    "build:zip:blog": "bash scripts/zip.sh --app=blog",
    "build:zip:wallpaper": "bash scripts/zip.sh --app=wallpaper",
    "build:zip:jigsaw": "bash scripts/zip.sh --app=jigsaw",
    "build:zip:game": "bash scripts/zip.sh --app=game",
    "build:win:zip": ".\\scripts\\build-zip.bat",
    "build:win:zip:www": ".\\scripts\\zip.bat --app=www",
    "build:win:zip:blog": ".\\scripts\\zip.bat --app=blog",
    "build:win:zip:wallpaper": ".\\scripts\\zip.bat --app=wallpaper",
    "build:win:zip:jigsaw": ".\\scripts\\zip.bat --app=jigsaw",
    "build:win:zip:game": ".\\scripts\\zip.bat --app=game",
    "watch:common": "ng build common --watch",
    "watch:www": "ng build www --watch --configuration development",
    "watch:blog": "ng build blog --watch --configuration development",
    "watch:wallpaper": "ng build wallpaper --watch --configuration development",
    "watch:jigsaw": "ng build jigsaw --watch --configuration development",
    "watch:game": "ng build game --watch --configuration development",
    "test": "ng test",
    "serve:ssr:www": "node dist/www/server/server.mjs",
    "serve:ssr:blog": "node dist/blog/server/server.mjs",
    "serve:ssr:wallpaper": "node dist/wallpaper/server/server.mjs",
    "serve:ssr:jigsaw": "node dist/jigsaw/server/server.mjs",
    "serve:ssr:game": "node dist/game/server/server.mjs",
    "lint": "eslint \"projects/**/*.{ts,html}\" --fix",
    "format": "prettier --write \"projects/**/*.ts\"",
    "deploy:zip": "bash scripts/deploy-zip-all.sh",
    "deploy:zip:www": "bash scripts/deploy-zip.sh --app=www",
    "deploy:zip:blog": "bash scripts/deploy-zip.sh --app=blog",
    "deploy:zip:wallpaper": "bash scripts/deploy-zip.sh --app=wallpaper",
    "deploy:zip:jigsaw": "bash scripts/deploy-zip.sh --app=jigsaw",
    "deploy:zip:game": "bash scripts/deploy-zip.sh --app=game",
    "deploy:win:zip": ".\\scripts\\deploy-zip-all.bat",
    "deploy:win:zip:www": ".\\scripts\\deploy-zip.bat --app=www",
    "deploy:win:zip:blog": ".\\scripts\\deploy-zip.bat --app=blog",
    "deploy:win:zip:wallpaper": ".\\scripts\\deploy-zip.bat --app=wallpaper",
    "deploy:win:zip:jigsaw": ".\\scripts\\deploy-zip.bat --app=jigsaw",
    "deploy:win:zip:game": ".\\scripts\\deploy-zip.bat --app=game",
    "deploy:local": "bash scripts/deploy-local.sh",
    "deploy:win:local": ".\\scripts\\deploy-local.bat",
    "deploy:remote": "bash scripts/deploy-remote.sh",
    "cloc": "cloc --exclude-dir=.git,.idea,.vscode,node_modules,dist,test,package-lock.json,.angular,.github,.nx ."
  },
  "dependencies": {
    "@angular/animations": "^20.2.2",
    "@angular/common": "^20.2.2",
    "@angular/compiler": "^20.2.2",
